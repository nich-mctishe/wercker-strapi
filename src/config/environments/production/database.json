{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-mongoose",
      "settings": {
        "client": "mongo",
        "host": "${process.env.DOKKU_MONGO_STRAPI_DB_PORT_27017_TCP_ADDR || '127.0.0.1'}",
        "port": "${process.env.DOKKU_MONGO_STRAPI_DB_PORT_27017_TCP_PORT || 27017}",
        "database": "strapi_db",
        "username": "${process.env.DB_USERNAME}",
        "password": "${process.env.DB_PASSWORD}"
      },
      "options": {}
    }
  }
}
